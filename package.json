{
  "name": "full-stack-project-1",
  "version": "1.0.0",
  "description": "Full Stack cource at Lexicon Institue",
  "repository": "https://github.com/mmallahham/FSProject1.git",
  "scripts": {
    "start": "npm-run-all --parallel sec-check open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "sec-check": "nsp check",
    "share": "lt -p 3000 --subdomain fsproject1",
    "lint": "esw buildScripts/*.js , src/js/*.js",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Mohammad Allahham",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.2",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "path": "^0.12.7",
    "style-loader": "^0.19.1",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "jsdom": "^11.5.1",
    "mocha": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  }
}
